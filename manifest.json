{
  "manifest_version": 2,

  "name": "RealSearch",
  "description": "This extension shows listings for a given address on Trulia, Zillow, and Realtor.com",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "popup.html"
  },
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  },
  "permissions": [
    "activeTab"
  ]
}